{
  "name": "docker_web_app",
  "version": "1.0.0",
  "description": "Node.js on Docker",
  "author": "Wes King <wes@boone.io>",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "kube-start": "minikube start --vm-driver virtualbox",
